<h1>Login</h1>

<%= form_for @conn, Routes.session_path(@conn, :create), [as: :session], fn f -> %>

<div>
  <%= text_input f, :username, placeholder: "Enter your username" %>
</div>

<div>
  <%= text_input f, :password, placeholder: "Enter your password" %>
</div>

<div>
  <%= submit "Login" %>
</div>

<% end %>
